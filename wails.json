{
    "$schema": "https://wails.io/schemas/config.v2.json",
    "name": "chat",
    "outputfilename": "chat",
    "frontend:install": "npm install",
    "frontend:build": "npm run build",
    "frontend:dev:watcher": "npm run dev",
    "frontend:dev:serverUrl": "auto",
    "frontend:dir": "./",
    "wailsjsdir": "./src/",
    "appargs": "-loglevel Info",
    "postBuildHooks": {
        "darwin/*": "cp ../llamacpp/server chat.app/Contents/MacOS/llama-server",
        "linux/*": "cp ../llamacpp/server llama-server"
    },
    "author": {
        "name": "Strange Bytes",
        "email": "dev@strangebytes.org"
    }
}
