{
    "name": "cybermuse",
    "version": "0.242.1",
    "main": "electron.js",
    "type": "module",
    "description": "A batteries included AI Chat Application",
    "author": "Strange Bytes",
    "scripts": {
        "dev": "vite --host",
        "build": "vite build",
        "build-electron": "bun run build-electron.ts",
        "make": "bun run build && bun run build-electron",
        "preview": "vite preview",
        "fmt": "dprint fmt",
        "lint": "oxlint && eslint",
        "typecheck": "vue-tsc -p ./src/frontend/tsconfig.json --noEmit",
        "gen-sync-types": "openapi-typescript http://localhost:31700/docs/json --output ./src/frontend/clients/sync-types.ts"
    },
    "devDependencies": {
        "@ai-sdk/openai-compatible": "^1.0.18",
        "@heroicons/vue": "^2.2.0",
        "@tailwindcss/vite": "^4.1.13",
        "@vitejs/plugin-vue": "^6.0.1",
        "@vue/eslint-config-typescript": "^14.6.0",
        "@vue/tsconfig": "^0.8.1",
        "ai": "^5.0.51",
        "daisyui": "^5.1.18",
        "dprint": "^0.50.2",
        "electron": "^38.1.2",
        "electron-builder": "^26.0.12",
        "eslint": "^9.36.0",
        "eslint-plugin-oxlint": "^1.18.0",
        "eslint-plugin-vue": "^10.5.0",
        "eventsource-parser": "^3.0.6",
        "idb": "^8.0.3",
        "jose": "^6.1.0",
        "liquidjs": "^10.21.1",
        "marked": "^16.3.0",
        "openapi-fetch": "^0.14.0",
        "openapi-typescript": "^7.9.1",
        "oxlint": "^1.18.0",
        "pinia": "^3.0.3",
        "pinia-plugin-persistedstate": "^4.5.0",
        "rollup-license-plugin": "^3.0.2",
        "tailwindcss": "^4.1.13",
        "vite": "^7.1.7",
        "vue": "^3.5.21",
        "vue-router": "^4.5.1",
        "vue-tsc": "^3.0.8",
        "zod": "^4.1.11",
        "zod-validation-error": "^4.0.2"
    }
}
