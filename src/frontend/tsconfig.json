{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": ["./**/*.ts", "./**/*.vue"],
    "compilerOptions": {
        "strict": true,
        "composite": true,
        "types": ["vite/client"],
        "baseUrl": ".",
        "paths": {
            "@/*": ["./*"]
        },
        "noEmit": true,
        "noUncheckedIndexedAccess": true
    }
}
